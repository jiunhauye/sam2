<h1>New domain</h1>

<form id="new-domain" name="domain">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value="<%= name %>" >
  </div>

  <div class="field">
    <label for="fullpath"> fullpath:</label>
    <input type="text" name="fullpath" id="fullpath" value="<%= fullpath %>" >
  </div>

  <div class="field">
    <label for="description"> description:</label>
    <input type="text" name="description" id="description" value="<%= description %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Domain" />
  </div>

</form>

